    "@types/parcel-env": "0.0.0",
    "@types/react": "^17.0.7",
    "@types/react-dom": "^17.0.5",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "parcel": "^2.2.1",
    "postcss": "^8.3.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-plugin": "^1.0.0",
    "postcss-url": "^10.1.3",
    "prettier": "2.5.1",
